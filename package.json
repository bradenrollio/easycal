{
  "name": "easycal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest --config .config/vitest.config.ts",
    "test:watch": "vitest --config .config/vitest.config.ts --watch",
    "test:coverage": "vitest --config .config/vitest.config.ts --coverage",
    "test:ui": "vitest --config .config/vitest.config.ts --ui",
    "db:push": "drizzle-kit push --config .config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config .config/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config .config/drizzle.config.ts",
    "prepare": "npm run type-check && npm run lint",
    "clean": "rm -rf .next out functions-dist node_modules/.cache",
    "organize": "npm run lint:fix && npm run type-check"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250903.0",
    "@gohighlevel/api-client": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-toast": "^1.2.15",
    "bottleneck": "^2.19.5",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-import": "^2.31.0",
    "happy-dom": "^15.11.6",
    "jsdom": "^25.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
